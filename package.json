{
  "name": "@moia-oss/bastion-host-forward",
  "description": "CDK Construct for creating a bastion host to forward a connection to several AWS data services inside a private subnet from your local machine",
  "author": {
    "name": "MOIA GmbH"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moia-oss/bastion-host-forward"
  },
  "keywords": [
    "cdk",
    "awscdk",
    "bastion host",
    "rds",
    "elasticache redis",
    "redshift",
    "aurora serverless"
  ],
  "license": "Apache-2.0",
  "version": "0.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak",
    "release-npm": "jsii-release-npm",
    "release-pypi": "jsii-release-pypi",
    "test": "jest",
    "lint": "tslint --project tsconfig.json --config tslint.yaml"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "moia-dev.bastion-host-forward",
        "module": "moia_dev.bastion_host_forward"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.134.0",
    "@types/jest": "^27.0.0",
    "@types/node": "16.11.12",
    "jest": "^26.6.3",
    "jsii": "^1.30.0",
    "jsii-pacmak": "^1.32.0",
    "jsii-release": "^0.2.30",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@aws-cdk/aws-ec2": "^1.134.0",
    "@aws-cdk/aws-elasticache": "^1.134.0",
    "@aws-cdk/aws-iam": "^1.134.0",
    "@aws-cdk/aws-rds": "^1.134.0",
    "@aws-cdk/aws-redshift": "^1.134.0",
    "@aws-cdk/core": "^1.134.0",
    "constructs": "^3.0.3"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.134.0",
    "@aws-cdk/aws-elasticache": "^1.134.0",
    "@aws-cdk/aws-iam": "^1.134.0",
    "@aws-cdk/aws-rds": "^1.134.0",
    "@aws-cdk/aws-redshift": "^1.134.0",
    "@aws-cdk/core": "^1.134.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  }
}
