{
  "name": "@moia-oss/bastion-host-forward",
  "description": "CDK Construct for creating a bastion host to forward a connection to several AWS data services inside a private subnet from your local machine",
  "author": {
    "name": "MOIA GmbH"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moia-oss/bastion-host-forward"
  },
  "keywords": [
    "cdk",
    "awscdk",
    "bastion host",
    "rds",
    "elasticache redis",
    "redshift",
    "aurora serverless"
  ],
  "license": "Apache-2.0",
  "version": "0.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak",
    "release-npm": "jsii-release-npm",
    "release-pypi": "jsii-release-pypi",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint ./lib/",
    "lint:fix": "eslint ./lib/ --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "moia-dev.bastion-host-forward",
        "module": "moia_dev.bastion_host_forward"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.19",
    "constructs": "10.4.2",
    "aws-cdk-lib": "2.196.1",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "jsii": "^5.0.3",
    "jsii-pacmak": "^1.52.1",
    "eslint-config-prettier": "^10.1.2",
    "typescript-eslint": "^8.30.1",
    "jsii-release": "^0.2.309",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "tslint": "^6.1.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "constructs": "^10.3.0",
    "aws-cdk-lib": "^2.121.1"
  },
  "dependencies": {}
}
